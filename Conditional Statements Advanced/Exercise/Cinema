function cinema(projectionType, rows, cols)
{
    let price = 0;
    let totalSpace = rows * cols;
    let income = 0;
    if(projectionType  == 'Premiere')
    {
        price = 12;
    }
    else if(projectionType  == 'Normal')
    {
        price = 7.5;
    }
    else if(projectionType  == 'Discount')
    {
        price = 5;
    }
    income = (totalSpace * price).toFixed(2);
    console.log(income);
}
