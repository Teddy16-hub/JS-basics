function workingHours(number, day) {
    if ((number >= 10 && number < 18) && (day === 'Monday' || day === 'Tuesday' || day === 'Wednesday' || day === 'Thursday' || day === 'Friday' || day === 'Saturday')) 
    {
        console.log('open');
    } 
    else 
    {
        console.log('closed');
    }
}
