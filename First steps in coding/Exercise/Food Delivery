function restaurantDelivery(chickenMenu, fishMenu, vegetarianMenu)
{
  let chickenMenuPrice = chickenMenu * 10.35;
  let fishMenuPrice = fishMenu * 12.4;
  let vegetarianMenuPrice = vegetarianMenu * 8.15;
  let totalMenuPrice = chickenMenuPrice + fishMenuPrice + vegetarianMenuPrice;
  let dessert = 0.2*totalMenuPrice;
  let totalPrice = totalMenuPrice + dessert + 2.5;
  console.log(totalPrice);
}
