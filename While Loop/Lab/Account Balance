function accoundBalance(input)
{
    let index = 0;
    let i = input[index];
    let sum = 0;
    while(i !== "NoMoreMoney")
    {
        let money = Number(i);
        if(money < 0)
        {
            console.log('Invalid operation!');
            break;
        }
        sum += money;
        console.log(`Increase: ${money.toFixed(2)}`);
        index++;
        i = input[index];
    }
    console.log(`Total: ${sum.toFixed(2)}`)
}
