function oldLibrary(input)
{
    let searchedBook = input[0];
    let index = 1;
    let bookSum = 0;
    while(input[index] !== 'No More Books')
    {
        let currBook = input[index];
        if(currBook == searchedBook)
        {
            console.log(`You checked ${bookSum} books and found it.`);
            return;
        }
        index++;
        currBook = input[index];
        bookSum++;
    }
    console.log('The book you search is not here!');
    console.log(`You checked ${bookSum} books.`);
}
