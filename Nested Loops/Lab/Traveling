function traveling(input)
{
    let index = 0;
    while(input[index] !== 'End')
    {
        let destination = input[index];
        index++;
        let budget = Number(input[index]);
        index++;
        let savings = 0;
        while(savings < budget)
        {
            let currSavings = Number(input[index]);
            savings += currSavings;
            index++;
        }
        console.log(`Going to ${destination}!`);
    }
}
